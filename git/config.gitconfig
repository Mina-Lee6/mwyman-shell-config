# Git config file for Michael Wyman
#
# Use this either directly or by adding the following lines to your standard .gitconfig
#
# [include]
#   path = [absolute or relative path to this file]
#
# [core]
#   excludesfile = [absolute or relative path to the config.gitignore next to this file]

[alias]
  st = status
  co = checkout
  ci = commit
  br = branch
  cp = cherry-pick
  pa = add --patch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short

[push]
  default = current
